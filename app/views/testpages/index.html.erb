<p id="notice"><%= notice %></p>

<h1>Testpages</h1>

<table>
  <thead>
    <tr>
      <th>Employeenumber</th>
      <th>Employeename</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @testpages.each do |testpage| %>
      <tr>
        <td><%= testpage.employeenumber %></td>
        <td><%= testpage.employeename %></td>
        <td><%= link_to 'Show', testpage %></td>
        <td><%= link_to 'Edit', edit_testpage_path(testpage) %></td>
        <td><%= link_to 'Destroy', testpage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Testpage', new_testpage_path %>
